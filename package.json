{
  "name": "suite-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run copy-cms-assets && next build",
    "start": "next start",
    "lint": "next lint",
    "gitlint": "./tools/run_gitlint.sh",
    "gitlint:install": "./tools/install_gitlint.sh",
    "format": "prettier --check \"**/*.{css,ts,tsx,md,json,cjs,js}\"",
    "format:fix": "prettier --write \"**/*.{css,ts,tsx,md,json,cjs,js}\"",
    "cms": "npx decap-server",
    "copy-cms-dsfr": "cp -r node_modules/@gouvfr/dsfr/dist/. public/assets/cms/dsfr && postcss public/assets/cms/dsfr/dsfr.min.css -o public/assets/cms/dsfr/dist/dsfr.min.css",
    "copy-cms-styles": "postcss src/styles/globals.css -o public/assets/cms/globals.css",
    "copy-cms-assets": "npm run copy-cms-dsfr && npm run copy-cms-styles",
    "postinstall": "npm run copy-cms-assets"
  },
  "dependencies": {
    "@bradgarropy/rehype-image-links": "1.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@gouvfr-lasuite/integration": "1.0.3",
    "@gouvfr/dsfr": "^1.13.0",
    "@headlessui/react": "^2.2.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "classnames": "2.5.1",
    "decap-cms-app": "3.8.4",
    "decap-server": "3.3.1",
    "grist-api": "0.1.7",
    "js-yaml": "4.1.0",
    "js-yaml-loader": "1.2.2",
    "next": "14.1.0",
    "postcss-cli": "11.0.1",
    "postcss-import": "16.1.1",
    "postcss-prefix-selector": "2.1.1",
    "probe-image-size": "7.2.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "10.1.0",
    "react-medium-image-zoom": "^5.3.0",
    "rehype-stringify": "10.0.1",
    "remark-gfm": "4.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "remark-unwrap-images": "5.0.0",
    "sharp": "0.34.4",
    "styled-components": "^6.1.15",
    "unified": "11.0.5",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.18.10",
    "@types/probe-image-size": "7.2.5",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3"
  }
}
